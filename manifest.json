{
    "name": "Cookie Copy",
    "version": "1.0.0",
    "description": "Copy paste cookies between web pages",
    "manifest_version": 3,
    "author": "Monica Macomber",
    "action":{
        "default_popup": "index.html",
        "default_title": "Cookie Copy",
        "default_icon": "cookie.png"
    },
    "host_permissions": ["<all_urls>"],
    "permissions": [
      "storage",
       "cookies"
    ]
}