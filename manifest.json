{
    "name": "Cookie Copy",
    "version": "1.0.0",
    "description": "Copy paste cookies between web pages",
    "manifest_version": 3,
    "author": "Monica Macomber",
    "action":{
        "default_popup": "index.html",
        "default_title": "Cookie Copy"

    },
    "host_permissions": ["<all_urls>"],
    "permissions": [
      "storage",
       "cookies",
       "tabs"
    ],
     "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
}